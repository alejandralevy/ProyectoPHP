{% extends "AppBundle::indexLayout.html.twig" %}

{% block body %}

<br>
<br>

<div class="well bs-component" style="width: 750px;border-radius: 12px;left: 0;right: 0;top:0;margin: auto;">
<form class="form-horizontal">
  <fieldset>
    <legend>
    Nuevo propietario
    
    </legend>
    	<div class="form-group ">
   	 		<div class="row">
  				<div class="col-xs-4" style="text-align: right;">
  					<label style="text-align: right; margin-bottom: 0;padding-top: 9px;">(*) Apellido</label>
  				</div>
 					<div class="col-xs-4 .col-xs-offset-4">
  				<input type="text" size="45" class="form-control" value="{{ propietario.apellido }}" autocomplete="off" id="apellido" placeholder="Apellido">
   					</div>
				</div>
			</div>
			<br>
			
  		<div class="form-group ">
   	 		<div class="row">
  				<div class="col-xs-4" style="text-align: right;">
  					<label style="text-align: right; margin-bottom: 0;padding-top: 9px;">(*) Nombre</label>
  				</div>
 			<div class="col-xs-4 .col-xs-offset-4">
  				<input type="text" size="45" class="form-control"  value="{{ propietario.nombre }}"autocomplete="off" id="nombre" placeholder="Nombre">
   			</div>
		</div>
	</div>
	
        <br>
           
    	<div class="form-group ">
   	 		<div class="row">
  				<div class="col-xs-4" style="text-align: right;">
  					<label style="text-align: right; margin-bottom: 0;padding-top: 9px;">(*) DNI</label>
  				</div>
 			<div class="col-xs-4 .col-xs-offset-4">
  				<input type="number" onkeypress='return event.charCode >= 48 && event.charCode <= 57' size="10" class="form-control" value="{{ propietario.dni }}"  autocomplete="off" id="dni"  placeholder="DNI">
   			</div>
		</div>
	</div>
		
		<br>
		
		<div class="form-group ">
   	 		<div class="row">
  				<div class="col-xs-4" style="text-align: right;">
  					<label style="text-align: right; margin-bottom: 0;padding-top: 9px;">(*) Lote</label>
  				</div>
 			<div class="col-xs-4 .col-xs-offset-4">
  				<input type="text" size="10" class="form-control" autocomplete="off" id="lote" value="{{ propietario.lote }}"  placeholder="Lote">
   			</div>
		</div>
	</div>
		
		<br>
		
		<div class="form-group ">
   	 		<div class="row">
  				<div class="col-xs-4" style="text-align: right;">
  					<label style="text-align: right; margin-bottom: 0;padding-top: 9px;">(*) Mail</label>
  				</div>
 			<div class="col-xs-4 .col-xs-offset-4">
  				<input type="text" size="10" class="form-control" value="{{ propietario.mail }}"  autocomplete="off" id="mail"  placeholder="Mail">
   			</div>
		</div>
	</div>
		
		<br>
		
		<div class="form-group ">
   	 		<div class="row">
  				<div class="col-xs-4" style="text-align: right;">
  					<label style="text-align: right; margin-bottom: 0;padding-top: 9px;">(*) Interno</label>
  				</div>
 			<div class="col-xs-4 .col-xs-offset-4">
  				<input type="number" size="10" class="form-control" value="{{ propietario.interno }}"  autocomplete="off" id="interno"  placeholder="Interno">
   			</div>
		</div>
	</div>
		
		<br>
		
		<div class="form-group ">
   	 		<div class="row">
  				<div class="col-xs-4" style="text-align: right;">
  					<label style="text-align: right; margin-bottom: 0;padding-top: 9px;">(*) Telefono 1</label>
  				</div>
 			<div class="col-xs-4 .col-xs-offset-4">
  				<input type="number" size="10" class="form-control" value="{{ propietario.telefono1 }}"  autocomplete="off" id="telefono1"  placeholder="Telefono 1">
   			</div>
		</div>
	</div>
		
		<br>
		
		<div class="form-group ">
   	 		<div class="row">
  				<div class="col-xs-4" style="text-align: right;">
  					<label style="text-align: right; margin-bottom: 0;padding-top: 9px;">Telefono 2</label>
  				</div>
 			<div class="col-xs-4 .col-xs-offset-4">
  				<input type="number" size="10" class="form-control" value="{{ propietario.telefono2 }}"  autocomplete="off" id="telefono2"  placeholder="Telefono 2">
   			</div>
		</div>
		</div>
		
		<br>
		
		
		
		<div class="form-group ">
   	 		<div class="row">
  				<div class="col-xs-4" style="text-align: right;">
  					<label style="text-align: right; margin-bottom: 0;padding-top: 9px;">Celular 1</label>
  				</div>
 			<div class="col-xs-4 .col-xs-offset-4">
  				<input type="number" size="10" class="form-control" value="{{ propietario.celular1 }}"  autocomplete="off" id="celular1"  placeholder="Celular 1">
   			</div>
		</div>
	</div>
		
		<br>
		
		

	<div class="form-group ">
   	 		<div class="row">
  				<div class="col-xs-4" style="text-align: right;">
  					<label style="text-align: right; margin-bottom: 0;padding-top: 9px;">Celular 2</label>
  				</div>
 			<div class="col-xs-4 .col-xs-offset-4">
  				<input type="number" class="form-control" value="{{ propietario.celular2 }}" id="celular2" placeholder="Celular 2">
   			</div>
		</div>
		</div>
	<br>
	
	<div class="form-group ">
   	 		<div class="row">
  				<div class="col-xs-4" style="text-align: right;">
  					<label style="text-align: right; margin-bottom: 0;padding-top: 9px;">(*) Fecha de Nacimiento</label>
  				</div>
 			<div class="col-xs-4 .col-xs-offset-4">
  				<input type="text" class="form-control" value="{{ propietario.fechaNacimiento|date('d/m/Y') }}" id="fechaNacimiento" placeholder="Fecha de Nacimiento">
   			</div>
		</div>
	</div>
	
		<br>
	
	<div class="form-group ">
   	 		<div class="row">
  				<div class="col-xs-4" style="text-align: right;">
  					<label style="text-align: right; margin-bottom: 0;padding-top: 9px;">Patente</label>
  				</div>
 			<div class="col-xs-4 .col-xs-offset-4">
  				<input type="text" size="10" class="form-control" value="{{ propietario.patente }}"  autocomplete="off" id="patente"  placeholder="Patente">
   			</div>
		</div>
		</div>
		
		<br>


<div class="form-group ">
   	 		<div class="row">
  				<div class="col-xs-4" style="text-align: right;">
  					<label style="text-align: right; margin-bottom: 0;padding-top: 9px;"> (*) Contrase&ntilde;a</label>
  				</div>
 			<div class="col-xs-4 .col-xs-offset-4">
  				<input type="password" class="form-control" value="{{ propietario.password }}"  autocomplete="off" id="password" required="required" placeholder="Contrase&ntilde;a">
   			</div>
		</div>
		</div>
		<br>

    <div class="form-group">
       <button type="button" id="cancelar" class="btn btn-primary btn-default">Cancelar</button>  &nbsp; &nbsp;
       <button type="button" id="submit" class="btn btn-primary btn-default">Aceptar</button>
     </div>
    </div>
</div>
</fieldset>
</form>
</div>


{% block body_js_jquery %}
	{{ parent() }}
	{% javascripts output='js/*.js' 
		'@AppBundle/Resources/public/js/propietarios/administrarPropietario.js'
	%}
	<script type="text/javascript" src="{{ asset_url }}"></script>
	{% endjavascripts %}
{% endblock %}

<script type="text/javascript">

$(document).ready(function() {

	$('.dropdown-toggle').dropdown();
	
	var rutas = new Rutas();
	
	rutas.rutaValidarPropietario = "{{ path('validarPropietario') }}"

	rutas.rutaPropietarios = "{{ path('propietarios') }}"

	rutas.id_usuario_editado = {{ propietario.id }}	
		
	rutas.init();

	
	

});



</script>

{% endblock %}
